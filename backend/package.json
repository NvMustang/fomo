{
    "name": "fomo-backend",
    "version": "1.0.0",
    "description": "Backend pour FOMO Beta - Google Sheets + ImgBB",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "migrate": "node migrate-all.js",
        "migrate:users": "node -e \"require('./migrate-all.js').migrateUsers().then(() => process.exit(0)).catch(e => { console.error(e); process.exit(1); })\"",
        "migrate:responses": "node -e \"require('./migrate-all.js').migrateResponses().then(() => process.exit(0)).catch(e => { console.error(e); process.exit(1); })\"",
        "add-images": "node add-event-images.js",
        "setup-drive": "node scripts/setup-google-drive.js",
        "check:sheets": "node scripts/check-sheets-references.js",
        "analyze:onboarding": "node scripts/analyze-onboarding-data.js",
        "analyze:onboarding:prod": "node scripts/analyze-onboarding-data.js --prod",
        "analyze:analytics": "node scripts/analyze-analytics-data.js",
        "analyze:analytics:prod": "node scripts/analyze-analytics-data.js --prod",
        "cleanup:analytics": "node scripts/cleanup-test-data.js",
        "test:geocoding": "node scripts/test-geocoding.js",
        "extract:image": "node scripts/extract-image-from-event.js",
        "extract:image:puppeteer": "node scripts/extract-image-puppeteer.js",
        "download:image": "node scripts/download-image.js",
        "get:image": "node scripts/get-image-from-event.js",
        "generate:image-dataset": "node scripts/generate-image-dataset.js",
        "upload:imgbb": "node scripts/upload-images-to-imgbb.js",
        "upload:and-update": "node scripts/upload-and-update-sheets.js"
    },
    "dependencies": {
        "axios": "^1.13.2",
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^4.22.1",
        "express-rate-limit": "^7.5.1",
        "googleapis": "^128.0.0",
        "helmet": "^7.2.0",
        "multer": "^1.4.5-lts.1",
        "puppeteer": "^24.30.0"
    },
    "devDependencies": {
        "nodemon": "^3.0.2"
    },
    "keywords": [
        "fomo",
        "upload",
        "google-drive",
        "images"
    ],
    "author": "FOMO Team",
    "license": "MIT"
}
